# Private settings in include file
[include]
  path = ~/.gitconfig.local

# Dracula theme
[url "https://github.com/dracula/"]
  insteadOf = dracula://
[color]
  ui = auto
[color "branch"]
  current = cyan bold reverse
  local = white
  plain =
  remote = cyan
[color "diff"]
  commit =
  func = cyan
  plain =
  whitespace = magenta reverse
  meta = white
  frag = cyan bold reverse
  old = red
  new = green
[color "grep"]
  context =
  filename =
  function =
  linenumber = white
  match =
  selected =
  separator =
[color "interactive"]
  error =
  header =
  help =
  prompt =
[color "status"]
  added = green
  changed = yellow
  header =
  localBranch =
  nobranch =
  remoteBranch = cyan bold
  unmerged = magenta bold reverse
  untracked = red
  updated = green bold
[user]
	name = Bots
	email = botsone@gmail.com
[credential] 
	helper = store
[credential "https://dev.azure.com"]
	useHttpPath = true
[alias]
	st = status
[core]
	editor = nano
